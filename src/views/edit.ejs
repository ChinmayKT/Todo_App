crossorigin="anonymous"></script>
    <title><%=data.title%></title>
</head>
<body>

    <nav class="navbar navbar-expand-lg navbar-light bg-light justify-content-between">
        <div class="container-fluid">
          <a class="navbar-brand" href="#">Dashboard</a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
              <li class="nav-item">
                <a class="nav-link active" >Email:<%=data.email%></a>
              </li>
             
              <li class="nav-item">
                <a class="nav-link " href="/logout"  >Logout</a>
              </li>
            </ul>
          </div>
        </div>
      </nav>
    <img src="<%=data.profile%>"" alt="profile" height="250px" width="250px">
    <br>
    <br>
    
    

    <div class="container w-50 p-3  bg-dark text-white">
        <h1>Edit Tasks</h1>
      
        <br>
        
        <ul class="list-group">
            
                <li class="list-group-item d-flex justify-content-between">
                    <form  action="/edit/todo/<%= etodo._id %>" method="POST"  >
                        <input type="text" name="todo" id="" class="form-control" placeholder="edit task" value="<%= etodo.todo %>" >
                        <br>
                        <button type="submit" class="btn btn-warning">  Edit Done</button>
            
                    </form> 
              
                   
                     

               
          </ul>




    
    
</body>
